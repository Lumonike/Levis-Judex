{
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/clean-jsdoc-theme",
        "destination": "./docs",
        "verbose": true,
        "readme": "./README.md",
        "theme_opts": {
            "default_theme": "dark"
        }
    },
    "markdown": {
        "hardwrap": false,
        "idInHeadings": true
    }
}
