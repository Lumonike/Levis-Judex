<%- include("partials/home-button") %>

<h1 class="text-4xl font-bold text-center mb-6 text-white">Contests List</h1>
<div>
    <% contests.forEach(contest => { %>
        <p class="text-2xl space-y-4 mb-4 text-center">
            <a class="hover:underline text-blue-400" href="/contests/<%= contest.id %>"><%= contest.id %>. <%= contest.name %></a>
        </p>
    <% }) %>
</div>
