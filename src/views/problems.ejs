<%- include("partials/home-button") %>

<h1 class="text-4xl font-bold text-center mb-6 text-white">Problems List</h1>
<div>
    <% problems.forEach(problem => { %>
        <p class="text-2xl space-y-4 mb-4 text-center">
            <a class="hover:underline text-blue-400" href="/problems/<%= problem.id %>"><%= problem.id %>. <%= problem.name %></a>
        </p>
    <% }) %>
</div>
