{
    "$schema": "https://typedoc.org/schema.json",
    "tsconfig": "./tsconfig.json",
    "entryPoints": ["src"],
    "entryPointStrategy": "expand",
    "out": "docs",
    "readme": "README.md",
    "logLevel": "Verbose",
    "highlightLanguages": ["bash", "console", "css", "html", "javascript", "json", "jsonc", "json5", "yaml", "tsx", "typescript", "dotenv"]
}
